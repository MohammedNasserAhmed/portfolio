{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [{ "key": "Cache-Control", "value": "public, max-age=600" }]
        }
    ]
}
