{
    "outputDirectory": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x",
            "maxDuration": 10,
            "memory": 128
        }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [{ "key": "Cache-Control", "value": "public, max-age=600" }]
        }
    ]
}
